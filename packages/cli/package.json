{
  "name": "@svarta/cli",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "devDependencies": {
    "@svarta/adapter-standalone": "workspace:*",
    "@svarta/eslint-config-custom": "workspace:*",
    "@svarta/tsconfig": "workspace:*",
    "@types/node": "18.11.17",
    "@types/yargs": "17.0.17",
    "eslint": "8.30.0",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "vitest": "0.26.2"
  },
  "engines": {
    "node": ">=14.18"
  },
  "dependencies": {
    "@svarta/core": "workspace:*",
    "@svarta/dev-server": "workspace:*",
    "chalk": "5.2.0",
    "yargs": "17.6.2",
    "zod": "3.20.2"
  },
  "peerDependencies": {
    "@svarta/adapter-standalone": "workspace:*"
  },
  "bin": {
    "svarta": "./bin.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}
