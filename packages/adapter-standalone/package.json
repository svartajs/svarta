    "vitest": "0.25.3"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "23.0.3",
    "@rollup/plugin-json": "5.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "10.0.1",
    "@svarta/core": "workspace:*",
    "@tinyhttp/app": "2.0.25",
    "chalk": "5.1.2",
    "esbuild": "0.15.18",
    "milliparsec": "2.2.2",
