{
  "name": "@svarta/adapter-standalone",
  "version": "0.0.12",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "fmt": "eslint \"**/*.ts\" --fix",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "devDependencies": {
    "@hono/node-server": "1.1.0",
    "@svarta/eslint-config-custom": "workspace:*",
    "@svarta/tsconfig": "workspace:*",
    "@tinyhttp/cookie": "2.0.5",
    "@types/node": "18.11.17",
    "eslint": "8.30.0",
    "hono": "3.3.0",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "vitest": "0.26.2",
    "yaml": "2.2.0"
  },
  "engines": {
    "node": "18 || 20"
  },
  "peerDependencies": {
    "@hono/node-server": "1.1.0",
    "hono": "3.3.0"
  },
  "dependencies": {
    "@svarta/core": "workspace:*",
    "chalk": "5.2.0",
    "esbuild": "0.16.10",
    "zod": "3.20.2"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
