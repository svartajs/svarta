{
  "name": "create-svarta-app",
  "version": "0.0.12",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "fmt": "eslint \"**/*.ts\" --fix",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "devDependencies": {
    "@svarta/eslint-config-custom": "workspace:*",
    "@svarta/tsconfig": "workspace:*",
    "@types/node": "18.11.17",
    "@types/prompts": "2.4.2",
    "@types/yargs": "17.0.17",
    "eslint": "8.30.0",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "vitest": "0.26.2"
  },
  "engines": {
    "node": "18 || 20"
  },
  "dependencies": {
    "@svarta/core": "workspace:*",
    "chalk": "5.2.0",
    "giget": "1.0.0",
    "prompts": "2.4.2"
  },
  "bin": "bin.mjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}
