    "typescript": "4.9.3",
    "vitest": "0.26.2"
  },
  "engines": {
    "node": ">=14.18"
  },
  "dependencies": {
    "detect-package-manager": "2.0.1",
    "walk-it": "1.1.1",
    "zod": "3.20.2"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
